def uniqueMorseRepresentations(words):
    m = [".-","-...","-.-.","-..",".","..-.","--.","....","..",
         ".---","-.-",".-..","--","-.","---",".--.","--.-",
         ".-.","...","-","..-","...-",".--","-..-","-.--","--.."]
    s = set()
    for w in words:
        s.add("".join(m[ord(c)-97] for c in w))
    return len(s)

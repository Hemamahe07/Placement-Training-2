def kSmallestPairs(nums1, nums2, k):
    import heapq
    h=[]
    for i in range(min(len(nums1),k)):
        heapq.heappush(h,(nums1[i]+nums2[0],i,0))
    res=[]
    while h and k:
        _,i,j=heapq.heappop(h)
        res.append([nums1[i],nums2[j]])
        if j+1<len(nums2):
            heapq.heappush(h,(nums1[i]+nums2[j+1],i,j+1))
        k-=1
    return res

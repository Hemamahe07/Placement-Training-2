class SubarrayProduct {
    public static void main(String[] args) {
        int[] a = {10,5,2,6};
        int k = 100, l = 0;
        long prod = 1;
        int count = 0;

        for (int r = 0; r < a.length; r++) {
            prod *= a[r];
            while (prod >= k && l <= r)
                prod /= a[l++];
            count += r - l + 1;
        }
        System.out.println(count);
    }
}

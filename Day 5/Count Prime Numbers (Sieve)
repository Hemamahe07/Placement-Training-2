class CountPrimes {
    public static void main(String[] args) {
        int n = 10;
        boolean[] p = new boolean[n];
        int count = 0;

        for (int i = 2; i < n; i++) {
            if (!p[i]) {
                count++;
                for (int j = i * 2; j < n; j += i)
                    p[j] = true;
            }
        }
        System.out.println(count);
    }
}

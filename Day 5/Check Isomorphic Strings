class Isomorphic {
    public static void main(String[] args) {
        String s = "egg", t = "add";
        int[] m1 = new int[256], m2 = new int[256];
        boolean ok = true;

        for (int i = 0; i < s.length(); i++) {
            if (m1[s.charAt(i)] != m2[t.charAt(i)])
                ok = false;
            m1[s.charAt(i)] = i + 1;
            m2[t.charAt(i)] = i + 1;
        }
        System.out.println(ok);
    }
}
